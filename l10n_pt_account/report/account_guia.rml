	          <para style="P5">[[ (o.partner_id and o.partner_id.vat) or removeParentNode('para') ]]</para>
	          <para style="P5">[[ o.sale_id.payment_term.name or removeParentNode('para') ]]</para>
	          <para style="P5">[[ o.stock_picking_id.name or removeParentNode('para') ]]</para>
	          </td>
          <td>
	          <para style="P5">[[ (o.partner_id and o.partner_id.title and o.partner_id.title.name) or '' ]] [[ (o.partner_id and o.partner_id.name) or '' ]]</para>
	          <para style="P5">[[ o.stock_picking_id.address_id and o.stock_picking_id.address_id.title.name or '' ]] [[ o.stock_picking_id.address_id and o.stock_picking_id.address_id.name or '' ]]</para>
	          <para style="P5">[[ o.stock_picking_id.address_id and o.stock_picking_id.address_id.street or removeParentNode('para') ]]</para>
	          <para style="P5">[[ (o.stock_picking_id .address_id and o.stock_picking_id.address_id.street2) or removeParentNode('para') ]]</para>
	          <para style="P5">[[ o.stock_picking_id.address_id and o.stock_picking_id.address_id.zip or '']] [[ o.stock_picking_id.address_id and o.stock_picking_id.address_id.city or '']]</para>
	          <para style="P5">[[ (o.stock_picking_id .address_id and o.stock_picking_id.address_id.state_id and o.stock_picking_id.address_id.state_id.name) or removeParentNode('para') ]]</para>
	          <para style="P5">[[ (o.stock_picking_id .address_id and o.stock_picking_id.address_id.country_id and o.stock_picking_id.address_id.country_id.name) or removeParentNode('para') ]]</para>
	          <para style="P5">Tel. : [[ (o.partner_id.address[0] and o.partner_id.address[0].phone) or removeParentNode('para') ]]</para>
	          <para style="P5">Fax : [[ (o.partner_id.address[0] and o.partner_id.address[0].fax) or removeParentNode('para') ]]</para>
